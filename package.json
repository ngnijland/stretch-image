{
  "name": "stretch-images",
  "version": "1.0.0",
  "description": "This is a project where you upload an image and get it back stretched and watermarked. Let your designers cringe!",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "./node_modules/.bin/tsc-watch -p tsconfig.build.json",
    "lint": "eslint \"src/**\"",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "yarn prettier:base --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "yarn prettier:base --write \"src/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngnijland/stretch-image.git"
  },
  "author": "Niek Nijland",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngnijland/stretch-image/issues"
  },
  "homepage": "https://github.com/ngnijland/stretch-image#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "prettier": "^1.18.2",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.4"
  }
}
